{
  "name": "employee-manager-node",
  "version": "0.0.1",
  "description": "Simple Node.js backend for employee-manager-react-native",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint . && tslint -p tsconfig.json && tsc --noEmit",
    "start": "nodemon --watch src --ext ts --exec ts-node ./src/server.ts",
    "prod": "yarn build && nodemon ./dist/server.js",
    "test": "echo \"Error: testing not configured\" && exit 1"
  },
  "author": {
    "name": "Luke Mayhew",
    "email": "luke@webuildlegends.com"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "dependencies": {
    "express": "^4.16.3"
  }
